<ion-header>
  <ion-toolbar color="dark">
    <ion-title>{{ pageTitle }}</ion-title>
    <ion-buttons slot="start">
      <ion-back-button></ion-back-button>
    </ion-buttons>
  </ion-toolbar>
</ion-header>

<ion-content padding class="background">
  <div *ngIf="byBusinessUnitDivFlag">
    <ion-list *ngFor="let data of listData" style="border-radius: 5px; margin-bottom: 5px;">
      <ion-row>
        <ion-col size="6" style="font-weight: bold;">
          Staff Id
        </ion-col>
        <ion-col size="6">
          {{ data.staff_no }}
        </ion-col>
      </ion-row>
      <ion-row>
        <ion-col size="6" style="font-weight: bold;">
          Associate Name
        </ion-col>
        <ion-col size="6">
          {{ data.staff_name }}
        </ion-col>
      </ion-row>
      <ion-row>
        <ion-col size="6" style="font-weight: bold;">
          Business Unit
        </ion-col>
        <ion-col size="6">
          {{ data.bu_name }}
        </ion-col>
      </ion-row>
    </ion-list>
  </div>
  <div *ngIf="byPracticetDivFlag">
    <ion-list *ngFor="let data of listData" style="border-radius: 5px; margin-bottom: 5px;">
