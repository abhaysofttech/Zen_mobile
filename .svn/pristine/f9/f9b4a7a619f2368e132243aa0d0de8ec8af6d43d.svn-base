.background{
    --background: linear-gradient(to top left, #99d6ff, #b3e0ff, #ccebff, white);
}

.row-style{
    justify-content: center;
    font-size: 20px;
    font-weight: bold;
    margin-top: 200px;
}

.search-button-style{
    margin-top: 30px;
    --background: #444;
    height: 50px;
}

.list-style{
    position: absolute;
    width: inherit;
    margin-right: 20px;
    overflow-y: scroll;
    max-height: 150%;
    z-index: 999;
}