.background{
    --background: linear-gradient(to top left, #99d6ff, #b3e0ff, #ccebff, white);
}

.image-row-style{
    width: 100%;
    justify-content: center;
    margin-top: 10px;
}

.image-style{
    width: 128px;
    height: 128px;
    border-radius: 100px;
    box-shadow: 0px 0px 4px rgba(0, 0, 0, 0.7);
}

.grid-style{
    margin-top: 50px;
    background: #D9D9D9;
}

.key-style{
    font-size: 14px;
    font-weight: bold;
}

.value-style{
    font-size: 14px;
}
